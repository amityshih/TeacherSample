<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.AlarmTimerService">
  <application android:icon="@drawable/icon">
    <activity android:label="@string/app_name" android:name=".AlarmTimer">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <service android:enabled="true" android:name=".AlarmTimerService" />
  </application>
 <uses-sdk android:minSdkVersion="7" />
</manifest>